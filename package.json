{
  "name": "odicee-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build:prod": "ng build --aot --build-optimizer --vendor-chunk",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "protractor e2e/protractor.conf.js ",
    "installWithFix": "npm install && node fixIE.js",
    "fixIE11": "node fixIE.js",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json -a screenshots --hideGenerator --theme vagrant   --open --serve",
    "webdriver:clean": "webdriver-manager clean",
    "webdriver:update": "webdriver-manager update --chrome --ie --gecko",
    "webdriver:start": "webdriver-manager start --detach --no-sandbox",
    "webdriver:stop": "webdriver-manager shutdown",
    "build-angular-for-prod": "node --max_old_space_size=6096 node_modules/@angular/cli/bin/ng build --prod --aot --build-optimizer --vendor-chunk --verbose --base-href=/ ",
    "build-angular-for-integ": "node --max_old_space_size=6096 node_modules/@angular/cli/bin/ng build --verbose --base-href=/ ",
    "fix-memory-limit": "cross-env LIMIT=6096 increase-memory-limit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "husky": {
    "hooks": {
      "pre-push": "ng build --prod"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.2.0",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.2.0",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "@auth0/angular-jwt": "^2.0.0",
    "@swimlane/ngx-datatable": "^13.0.1",
    "angular6-json-schema-form": "^1.0.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "mydatepicker": "^2.6.3",
    "ngx-page-scroll": "^5.0.0",
    "node-sass": "^4.9.3",
    "rxjs": "^6.0.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@compodoc/compodoc": "^1.1.3",
    "@types/chai": "^4.1.3",
    "@types/cucumber": "^4.0.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "~4.2.1",
    "cross-env": "^5.0.5",
    "cucumber": "^4.2.1",
    "husky": "^1.0.0-rc.8",
    "increase-memory-limit": "^1.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.13.4",
    "pretty-quick": "^1.6.0",
    "protractor-cucumber-framework": "^5.0.0",
    "jasmine": "^2.8.0",
    "protractor": "^5.2.1",
    "protractor-jasmine2-screenshot-reporter": "^0.5.0",
    "sass-loader": "^7.0.1",
    "stylelint": "^9.3.0",
    "ts-node": "^6.0.5",
    "tslint": "~5.9.1",
    "typescript": "2.9.2"
  },
  "snyk": true
}
